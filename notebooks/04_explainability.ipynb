    "# Define sample texts with different emotional content\n",
    "sample_texts = {\n",
    "    \"joy_example\": \"I just got accepted to my dream university! This is the best day of my life and I couldn't be happier!\",\n",
    "    \"sadness_example\": \"I'm feeling really down today. Everything seems to be going wrong and I don't know what to do.\",\n",
    "    \"anger_example\": \"I can't believe they cancelled my flight again! This is absolutely ridiculous and unacceptable!\",\n",
    "    \"mixed_emotions\": \"I'm excited about the new job opportunity, but also nervous about leaving my current company and colleagues.\",\n",
    "    \"subtle_emotion\": \"The weather is nice today, I guess.\",\n",
    "    \"complex_narrative\": \"After years of hard work and countless rejections, I finally achieved my goal. The journey was painful but the success feels incredible now.\"\n",
    "}\n",
    "\n",
    "def get_model_predictions(text: str, top_k: int = 5) -> Dict[str, float]:\n",
    "    \"\"\"Get top-k predictions for a text.\"\"\"\n",
    "    result = shap_explainer.explain_text(text)\n",
    "    predictions = result.predictions\n",
    "    \n",
    "    # Sort by prediction score\n",
    "    sorted_preds = sorted(predictions.items(), key=lambda x: x[1], reverse=True)\n",
    "    return dict(sorted_preds[:top_k])\n",
    "\n",
    "# Get predictions for all sample texts\n",
    "print(\"üîÆ Model Predictions for Sample Texts\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "for name, text in sample_texts.items():\n",
    "    print(f\"\\nüìÑ {name.upper().replace('_', ' ')}:\")\n",
    "    print(f\"Text: {text}\")\n",
    "    print(\"\\nTop 5 Predictions:\")\n",
    "    \n",
    "    preds = get_model_predictions(text)\n",
    "    for emotion, score in preds.items():\n",
    "        print(f\"  {emotion:>12}: {score:.3f} {'‚ñà' * int(score * 20)}\")"
   ]
