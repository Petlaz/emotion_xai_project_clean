    "# Interactive Explanation Function\n",
    "def explain_emotion_text(text: str) -> None:\n",
    "    \"\"\"Clean, focused explanation function using LIME.\"\"\"\n",
    "    \n",
    "    print(f\"üîç Analyzing: {text}\")\n",
    "    print(\"-\" * 60)\n",
    "    \n",
    "    # Get predictions using LIME (our reliable method)\n",
    "    lime_result = lime_explainer.explain_top_emotions(text, top_k=3, num_features=5)\n",
    "    \n",
    "    # Show top predictions\n",
    "    print(f\"\\nüéØ Top Predictions:\")\n",
    "    sorted_preds = sorted(lime_result.predictions.items(), key=lambda x: x[1], reverse=True)[:3]\n",
    "    for emotion, score in sorted_preds:\n",
    "        bar = \"‚ñà\" * int(score * 20)\n",
    "        print(f\"  {emotion:>15}: {score:.3f} {bar}\")\n",
    "    \n",
    "    # Show feature importance for top emotion\n",
    "    print(f\"\\nüçã Key Influences (LIME):\")\n",
    "    if lime_result.feature_importance and sorted_preds:\n",
    "        top_emotion = sorted_preds[0][0]\n",
    "        if top_emotion in lime_result.feature_importance:\n",
    "            importance = lime_result.feature_importance[top_emotion]\n",
    "            sorted_features = sorted(importance.items(), key=lambda x: abs(x[1]), reverse=True)[:5]\n",
    "            for word, score in sorted_features:\n",
    "                direction = \"üìà\" if score > 0 else \"üìâ\"\n",
    "                print(f\"    {word:>12}: {score:>6.3f} {direction}\")\n",
    "    print()\n",
    "\n",
    "# Demonstration with focused examples\n",
    "demo_texts = [\n",
    "    \"I love spending time with my family during the holidays.\",\n",
    "    \"This movie was absolutely terrible and disappointing.\",\n",
    "    \"Congratulations on your achievement! You deserve it.\",\n",
    "    \"I'm not sure how I feel about this uncertain situation.\"\n",
    "]\n",
    "\n",
    "print(\"üé≠ Interactive Explanation Demonstrations\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "for i, text in enumerate(demo_texts, 1):\n",
    "    print(f\"\\nüìÑ Example {i}:\")\n",
    "    explain_emotion_text(text)\n",
    "    \n",
    "print(\"‚ú® Demonstrations completed!\")\n",
    "print(\"üí° This function can analyze any custom text for emotions and explanations.\")"
